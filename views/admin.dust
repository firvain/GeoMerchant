<!DOCTYPE html>
<html lang={lang}>

  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{title}</title>
    <link href="https://fonts.googleapis.com/css?family=Ubuntu:400,300,300italic,400italic,500,500italic,700,700italic&subset=latin,cyrillic-ext,latin-ext,cyrillic,greek,greek-ext" rel="stylesheet" type="text/css">
    <!-- <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/meyer-reset/2.0/reset.min.css"> -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="https://code.getmdl.io/1.1.1/material.purple-indigo.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ol3/3.13.1/ol.min.css" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" crossorigin="anonymous"> {!
    <link rel="stylesheet" href="/vendor/getmdl-select/getmdl-select.min.css" crossorigin="anonymous"> !}
    <link rel="stylesheet" href="/vendor/mdl-selectfield-master/mdl-selectfield.min.css" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/admin.min.css" crossorigin="anonymous"> </head>

  <body>
    <div class="appwrapper">
    <div class="spiner-wrapper">
      
      <div class="mdl-spinner mdl-spinner--single-color mdl-js-spinner  mdl-spinner-big"></div>
    </div>
      <div id="openModal" class="modal-dialog mdl-shadow--16dp visuallyhidden">
        <div class="modal-content"> </div>
      </div>
      <div class="adminMap" id="adminMap"> </div>
      <div class="infobox mdl-shadow--8dp">
        <div class="mdl-grid">
          <div class="mdl-cell mdl-cell--12-col">
            <div class="userinfo">
              <p>Welcome {user.nickname}!</p>
            </div>
          </div>
        </div>
        <div class="mdl-grid">
          <div class="mdl-cell mdl-cell--12-col actions">
            <button id="insertProperty" class="mdl-button mdl-js-button mdl-button--primary mdl-js-ripple-effect"> {btns.insert} </button>
            <button id="deleteProperty" class="mdl-button mdl-js-button mdl-button--primary mdl-js-ripple-effect"> {btns.delete} </button>
            <button id="updateProperty" class="mdl-button mdl-js-button mdl-button--primary mdl-js-ripple-effect"> {btns.update} </button>
            <button id="logout" class="mdl-button mdl-js-button mdl-js-ripple-effect "> {btns.logout} </button>
          </div>
        </div>
        <div class="mdl-grid property-info visuallyhidden">
          <div class="mdl-cell mdl-cell--12-col ">
            <div class="property-info visuallyhidden"> </div>
          </div>
        </div>
      </div>
    </div>
    <!-- jQuery -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/ol3/3.13.1/ol.min.js" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/dustjs-linkedin/2.7.2/dust-full.min.js" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/dustjs-helpers/1.7.3/dust-helpers.min.js" crossorigin="anonymous"></script>
    <script src="https://code.getmdl.io/1.1.1/material.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/lodash.js/4.1.0/lodash.min.js" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.1/moment.min.js" crossorigin="anonymous"></script>
    <script src="/vendor/parsley/parsley.min.js" crossorigin="anonymous"></script>
    <script src="/vendor/mdl-selectfield-master/mdl-selectfield.min.js" crossorigin="anonymous"></script> {!
    <script src="/vendor/getmdl-select/getmdl-select.min.js" crossorigin="anonymous"></script> !} {! ======= Templates ======= !}
    <script src="/js/tpl/adminPropertyInfo.js" crossorigin="anonymous"></script>
    <script src="/js/tpl/propertyInsert.js" crossorigin="anonymous"></script>
    <script src="/js/tpl/propertyUpdate.js" crossorigin="anonymous"></script>
    <script src="/js/tpl/listingInsert.js" crossorigin="anonymous"></script>
    <script src="/js/tpl/dialog.js" crossorigin="anonymous"></script>
    <script type="text/javascript">
    var id = parseInt('{id}');
    </script>
    <script src="/js/admin.min.js" crossorigin="anonymous"></script>
  </body>

</html>
