{
  "rules": {
    "indent": [
      2,
      2,
      {"VariableDeclarator": 2}
    ],
    "quotes": [
      1, "single"
    ],
    "semi": [
      2, "always"
    ],
    "consistent-this": [2, "self"],
    "camelcase": [2, {
      "properties": "always"
    }],
    "block-scoped-var": [2],
    "no-loop-func": [2],
    "no-console": [1],
    "no-multi-spaces": [1],
    "no-unused-vars": [1, {
      "vars": "local",
      "args": "after-used"
    }],
    "no-undef": 1
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jquery": true
  },
  "globals": {
    "ol": true,
    "dust": true,
    "toastr":true
  },
  "plugins": ["html"],
  "extends": "eslint:recommended"
}
